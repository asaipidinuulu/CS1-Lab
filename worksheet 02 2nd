       #1
for i in range(2000,3200):
  if i%7==0 and i%5!=0:
    print(i)
       
       #2
x=int(input("give a number:"))
factorial=1
for i in range(1,x + 1):
  factorial = factorial*i
  print(factorial, end=",")
       
       #3
n=int(input("write a number:"))
d=dict()
for i in range(1,n+1):
  d[i]=i*i

print(d)
