        #1
def func1(*argument):
    for i in argument:
        print(i)

func1(12,4,5,6,7)
func1(56,45,34,7,55)
        
        #2
def recursive(n): 
  if n <= 1: 
    return n 
  else:
    return n + recursive(n - 1) 
  

print(recursive(10))
  
        #3
string="pynative"
print(string[::2])

        #4
ints=[2,45,6,4,34,5,64,3]
def x(ints):
  if ints[0]==ints[-1]:
    return True
  else:
    return False
result=(x(ints))
print(result)
